{
  "name": "chrome-headless-render-pdf",
  "version": "1.10.0",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/Szpadel/chrome-headless-render-pdf",
  "engineStrict": true,
  "bin": {
